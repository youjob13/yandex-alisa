        "build": "tsc",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier . --write"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/youjob13/yandex-alisa.git"
    },
